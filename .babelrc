{
  "presets": [
    ["@babel/preset-env", {
      "targets": { "browsers": ["last 2 versions"] },
      "modules": false
    }],
    ["@babel/preset-stage-2", {
      "decoratorsLegacy": true
    }],
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
